{
	"name": "clade",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"runArgs": [
		"--privileged"
	],
	"extensions": [
		"ms-azuretools.vscode-docker",
		"golang.go",
		"streetsidesoftware.code-spell-checker"
	],
	"postStartCommand": "nohup bash -c 'sudo dockerd-entrypoint.sh dockerd --group hypnos &' > ~/.dockerd.log 2>&1"
}
